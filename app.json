{
  "expo": {
    "name": "MAD",
    "description": "Progetto per LAP2",
    "slug": "mad",
    "privacy": "unlisted",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android"],
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "notification": {
      "icon": "./assets/notification_icon.png",
      "androidMode": "default"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.emme.mad"
    },
    "android": {
      "package": "com.emme.mad",
      "googleServicesFile": "./src/api/google-services.json"
    }
  }
}